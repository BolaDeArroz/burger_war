<?xml version="1.0"?>
<launch>
  <arg name="robot_name" default=""/>
    <!-- You can change control program  -->

    <!-- Launch Navigation -->
    <include file="$(find burger_navigation)/launch/your_burger_navigation_sample.launch">
        <arg name="robot_name" value="$(arg robot_name)" />
    </include>

    <!-- my program node -->
    <node pkg="burger_war" type="bola_de_arroz.py" name="AttackRunBot" output="screen"> 
        <param name="robot_name" value="$(arg robot_name)" />
    </node>
    <node pkg="burger_sakuteki" type="burger_sakuteki" name="Sakuteki" output="screen"> 
        <param name="robot_name" value="$(arg robot_name)" />
    </node>
    <node pkg="burger_sakuteki" type="camera_subscriber.py" name="image_subscriber" output="screen"> 
        <param name="robot_name" value="$(arg robot_name)" />
        <param name="in_image" value="/$(arg robot_name)/image_raw" />
    </node>



    <!-- END my program node -->

    <!-- End of your space  -->
</launch>

